#Raccourcis universels
super + a
	dmenu_run -c
super + shift + e
	dpower
super + m
	dmount
super + u
	dumount
super + shift + l
	slock
XF86AudioNext
	mpc next
XF86AudioPrev
	mpc prev
XF86AudioPlay
	mpc toggle
XF86AudioMute
	pamixer -t
XF86AudioRaiseVolume
	pamixer -i 2
XF86AudioLowerVolume
	pamixer -d 2
XF86AudioStop
	mpc clear
XF86MonBrightnessDown
	xbacklight -dec 10
XF86MonBrightnessUp
	xbacklight -inc 10
