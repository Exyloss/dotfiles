#Raccourcis universels
super + shift + e
	dpower
#super + q
#	xdo close
super + a
	dmenu_run -c
super + m
	dmount
super + u
	dumount
super + shift + l
	slock
super + w
	"$TRUEBROWSER"
super + e
	"$TERMINAL" -e lf
super + shift + Return
	"$TERMINAL"
Print
	"$TERMINAL" -e fzopen
Pause
	mpc -q toggle
Home
	dpower
super + shift + {p,n}
	mpc -q {prev,next}
XF86Audio{Prev,Next}
	mpc -q {prev,next}
XF86AudioPlay
	mpc -q toggle
XF86AudioMute
	pamixer -t; kill -45 $(pidof dwmblocks)
XF86Audio{Raise,Lower}Volume
	pamixer -{i,d} 2; kill -45 $(pidof dwmblocks)
XF86AudioStop
	mpc clear
XF86MonBrightness{Down,Up}
	xbacklight -{dec,inc} 10
