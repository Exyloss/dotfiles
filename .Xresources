!! Transparency (0-1):
st.alpha: 0.92

!! Set a default font and font size as below:
st.font: Monospace-11;
URxvt*scrollBar:	    false
URxvt*letterSpace:  	-3
URxvt.internalBorder: 10
URxvt*font: 		      xft:Menlo for Powerline:regular:size=10
URxvt*boldFont:		    xft:Menlo for Powerline:bold:size=10
URxvt*cursorBlink:    true
URxvt.iso14755:		    false
URxvt*utf8:			      true
URxvt.keysym.C-equal:      resize-font:bigger
URxvt.keysym.C-parenright: resize-font:smaller
urxvt*depth: 32
urxvt*background: rgba:0000/0000/0000/c800

